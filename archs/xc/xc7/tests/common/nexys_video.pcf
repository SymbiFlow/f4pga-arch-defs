# nexys-video 100 MHz CLK
set_io clk R4

# sw[0:7] correspond with SW0-SW7 on the nexys-video
set_io sw[0] E22
set_io sw[1] F21
set_io sw[2] G21
set_io sw[3] G22
set_io sw[4] H17
set_io sw[5] J16
set_io sw[6] K13
set_io sw[7] M17

# led[0:7] correspond with LD0-LD7 on the nexys-video
set_io led[0] T14
set_io led[1] T15
set_io led[2] T16
set_io led[3] U16
set_io led[4] V15
set_io led[5] W16
set_io led[6] W15
set_io led[7] Y13

# UART bridge
set_io tx AA19
set_io rx V18
