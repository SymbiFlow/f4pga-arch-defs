<?xml version="1.0"?>
<!--
  Convert
     <pack_pattern name="xxx" type="yyy
  to
     <pack_pattern name="yyy-xxx"

  The idea is that eventually VPR will be explicit about chain and molecule
  patterns.

  -->
<xml>
 <pb_type>
  <direct>
   <pack_pattern name="B" type="chain" />
  </direct>
  <mux>
   <pack_pattern name="A" type="molecule" >
    <innertag />
   </pack_pattern>
  </mux>
 </pb_type>
</xml>
