<!-- set: ai sw=1 ts=1 sta et -->
<pb_type name="BLK_TL-PIO" xmlns:xi="http://www.w3.org/2001/XInclude">
 <!-- SB_IO inputs   -->
 <output name="D_IN_0" num_pins="2" equivalent="false"/>
 <output name="D_IN_1" num_pins="2" equivalent="false"/>

 <!-- SB_IO outputs -->
 <input  name="D_OUT_0" num_pins="2" equivalent="false"/>
 <input  name="D_OUT_1" num_pins="2" equivalent="false"/>

 <!-- Control signals -->
 <input  name="OUT_ENB" num_pins="2" equivalent="false"/>

 <!--
 <input  name="io_global_cen"    num_pins="1" equivalent="false"/>
 <clock  name="io_global_inclk"  num_pins="1" equivalent="false"/>
 <clock  name="io_global_outclk" num_pins="1" equivalent="false"/>
 <input  name="io_global_latch"  num_pins="1" equivalent="false"/>
 -->
 <output name="PACKAGE_PIN_O" num_pins="2" equivalent="false"/>
 <input name="PACKAGE_PIN_I" num_pins="2" equivalent="false"/>

 <pb_type name="BLK_IG-IO" num_pb="2">
  <output name="D_IN_0" num_pins="1" equivalent="false"/>
  <output name="D_IN_1" num_pins="1" equivalent="false"/>

  <!-- SB_IO outputs -->
  <input  name="D_OUT_0" num_pins="1" equivalent="false"/>
  <input  name="D_OUT_1" num_pins="1" equivalent="false"/>

  <!-- Control signals -->
  <input  name="OUT_ENB" num_pins="1" equivalent="false"/>

  <!--
  <input  name="io_global_cen"    num_pins="1" equivalent="false"/>
  <clock  name="io_global_inclk"  num_pins="1" equivalent="false"/>
  <clock  name="io_global_outclk" num_pins="1" equivalent="false"/>
  <input  name="io_global_latch"  num_pins="1" equivalent="false"/>
  -->
  <output name="PACKAGE_PIN_O" num_pins="1" equivalent="false"/>
  <input name="PACKAGE_PIN_I" num_pins="1" equivalent="false"/>
  <!-- IO operating as an output -->
  <mode name="PAD_IS_OUTPUT">
   <pb_type name="OUTPUT" num_pb="1">
    <input name="I" num_pins="1" equivalent="false"/>
    <output name="O" num_pins="1" equivalent="false"/>
    <interconnect>
     <direct name="USED" input="OUTPUT.I" output="OUTPUT.O"/>
    </interconnect>
    <metadata>
     <meta name="hlc_property">disable_pull_up</meta>
     <meta name="hlc_property">output_pin_type = simple_output_pin</meta>
     <meta name="hlc_property">input_pin_type = simple_input_pin</meta>
    </metadata>
   </pb_type>
   <interconnect>
    <direct name="I" input="BLK_IG-IO.D_OUT_0" output="OUTPUT.I" />
    <direct name="O" input="OUTPUT.O" output="BLK_IG-IO.PACKAGE_PIN_O" />
   </interconnect>
  </mode>

  <!-- IO operating as an input -->
  <mode name="PAD_IS_INPUT">
   <pb_type name="INPUT" num_pb="1">
    <input name="I" num_pins="1" equivalent="false"/>
    <output name="O" num_pins="1" equivalent="false"/>
    <interconnect>
     <direct name="USED" input="INPUT.I" output="INPUT.O"/>
    </interconnect>
    <metadata>
     <meta name="hlc_property">disable_pull_up</meta>
     <meta name="hlc_property">enable_input</meta>
     <meta name="hlc_property">input_pin_type = simple_input_pin</meta>
    </metadata>
   </pb_type>
   <interconnect>
    <direct name="I" input="BLK_IG-IO.PACKAGE_PIN_I" output="INPUT.I" />
    <direct name="O" input="INPUT.O" output="BLK_IG-IO.D_IN_0" />
   </interconnect>
  </mode>
 </pb_type>

 <interconnect>
  <direct name="BLK_IG-IO[0].D_OUT_0"        input="BLK_TL-PIO.D_OUT_0[0]"       output="BLK_IG-IO[0].D_OUT_0"        />
  <direct name="BLK_IG-IO[0].D_OUT_1"        input="BLK_TL-PIO.D_OUT_1[0]"       output="BLK_IG-IO[0].D_OUT_1"        />
  <direct name="BLK_IG-IO[0].OUT_ENB"        input="BLK_TL-PIO.OUT_ENB[0]"       output="BLK_IG-IO[0].OUT_ENB"        />
  <direct name="BLK_TL-PIO.D_IN_0[0]"        input="BLK_IG-IO[0].D_IN_0"         output="BLK_TL-PIO.D_IN_0[0]"        />
  <direct name="BLK_TL-PIO.D_IN_1[0]"        input="BLK_IG-IO[0].D_IN_1"         output="BLK_TL-PIO.D_IN_1[0]"        />
  <direct name="BLK_IG-IO[0].PACKAGE_PIN_I"  input="BLK_TL-PIO.PACKAGE_PIN_I[0]" output="BLK_IG-IO[0].PACKAGE_PIN_I"  />
  <direct name="BLK_TL-PIO.PACKAGE_PIN_O[0]" input="BLK_IG-IO[0].PACKAGE_PIN_O"  output="BLK_TL-PIO.PACKAGE_PIN_O[0]" />

  <direct name="BLK_IG-IO[1].D_OUT_0"        input="BLK_TL-PIO.D_OUT_0[1]"       output="BLK_IG-IO[1].D_OUT_0"        />
  <direct name="BLK_IG-IO[1].D_OUT_1"        input="BLK_TL-PIO.D_OUT_1[1]"       output="BLK_IG-IO[1].D_OUT_1"        />
  <direct name="BLK_IG-IO[1].OUT_ENB"        input="BLK_TL-PIO.OUT_ENB[1]"       output="BLK_IG-IO[1].OUT_ENB"        />
  <direct name="BLK_TL-PIO.D_IN_0[1]"        input="BLK_IG-IO[1].D_IN_0"         output="BLK_TL-PIO.D_IN_0[1]"        />
  <direct name="BLK_TL-PIO.D_IN_1[1]"        input="BLK_IG-IO[1].D_IN_1"         output="BLK_TL-PIO.D_IN_1[1]"        />
  <direct name="BLK_IG-IO[1].PACKAGE_PIN_I"  input="BLK_TL-PIO.PACKAGE_PIN_I[1]" output="BLK_IG-IO[1].PACKAGE_PIN_I"  />
  <direct name="BLK_TL-PIO.PACKAGE_PIN_O[1]" input="BLK_IG-IO[1].PACKAGE_PIN_O"  output="BLK_TL-PIO.PACKAGE_PIN_O[1]" />
 </interconnect>

 <metadata>
  <meta name="hlc_cell">io</meta>
  <meta name="hlc_tile">io_tile</meta>
 </metadata>

 <pinlocations pattern="custom">
  <loc side="right" xoffset="0" yoffset="0">
   BLK_TL-PIO.D_IN_0
   BLK_TL-PIO.D_IN_1
   BLK_TL-PIO.D_OUT_0
   BLK_TL-PIO.D_OUT_1
   BLK_TL-PIO.OUT_ENB
   BLK_TL-PIO.PACKAGE_PIN_O
   BLK_TL-PIO.PACKAGE_PIN_I
  </loc>
 </pinlocations>
 <fc in_type="frac" in_val="1.0" out_type="frac" out_val="1.0" />
</pb_type>
