    <direct name="WDATA05" input="BLK_TL-RAMT.WDATAT[5]" output="SB_RAM.WDATA[5]"/>
    <direct name="WDATA06" input="BLK_TL-RAMT.WDATAT[6]" output="SB_RAM.WDATA[6]"/>
