     BLK_TL-RAMT.RDATAB
     BLK_TL-RAMT.WADDR
     BLK_TL-RAMT.MASKB
     BLK_TL-RAMT.WDATAB
     BLK_TL-RAMT.WCLKE
     BLK_TL-RAMT.WCLK
     BLK_TL-RAMT.WE
     BLK_TL-RAMT.RDATAT
     BLK_TL-RAMT.RADDR
     BLK_TL-RAMT.MASKT
     BLK_TL-RAMT.WDATAT
     BLK_TL-RAMT.RCLKE
     BLK_TL-RAMT.RCLK
     BLK_TL-RAMT.RE
    </loc>
   </pinlocations>
   <!--
    </loc>
    <loc side="right" xoffset="0" yoffset="1"> -->
   <switchblock_locations pattern="external_full_internal_straight"/>
